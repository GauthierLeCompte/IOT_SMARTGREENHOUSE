{"ast":null,"code":"export default {\n  name: \"GreenhouseDashboard\"\n};","map":{"version":3,"names":["name"],"sources":["src/components/GreenhouseDashboard.vue"],"sourcesContent":["<template>\n  <div class=\"jumbotron vertical-center\">\n  <div class=\"container\">\n    <!-- bootswatch CDN -->\n    <link rel=\"stylesheet\" href=\"https://cdn.jsdelivr.net/npm/bootswatch@4.5.2/dist/lux/bootstrap.min.css\"\n          integrity=\"sha384-9+PGKSqjRdkeAU7Eu4nkJU8RFaH8ace8HGXnkiKMP9I9Te0GJ4/km3L1Z8tXigpG\"\n          crossorigin=\"anonymous\">\n    <div class=\"row\">\n      <div class=\"col-sm-12\">\n        <h1>Greenhouse Control Dashboard üè°</h1>\n        <hr><br>\n\n        <!-- Alert message when somethings has changed -->\n        <label>Set desired temperature:</label>\n        <input type=\"number\" v-model=\"temperature\"/><br><br>\n\n        <label>Set Lights:</label>\n        <div class=\"btn-group\" role=\"group\">\n          <button type=\"button\" class=\"btn btn-success\">On</button>\n          <button type=\"button\" class=\"btn btn-danger\">Off</button>\n        </div><br><br>\n\n        <label>Set desired humidity:</label>\n        <input type=\"number\" v-model=\"temperature\"/><br><br>\n\n        <label>Set desired pressure:</label>\n        <div class=\"btn-group\" role=\"group\">\n          <button type=\"button\" class=\"btn btn-success\">Low</button>\n          <button type=\"button\" class=\"btn btn-info\">Medium</button>\n          <button type=\"button\" class=\"btn btn-danger\">High</button>\n        </div><br><br>\n\n        <br><br>\n        <table class=\"table table-hover\">\n          <thead>\n            <tr>\n              <!-- table header cells -->\n              <th scope=\"col\">Temperature</th>\n              <th scope=\"col\">Lights</th>\n              <th scope=\"col\">Humidity</th>\n              <th scope=\"col\">Pressure</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"(game, index) in games\" :key=\"index\">\n              <!-- td : table data -->\n              <td>{{game.title}}</td>\n              <td>{{game.genre}}</td>\n              <td>\n                <span v-if=\"game.played\">Yes</span>\n                <span v-else>No</span>\n              </td>\n              <td>\n                <div class=\"btn-group\" role=\"group\">\n                   <!-- 2 Handle update button click -->\n                <button\n                  type=\"button\"\n                  class=\"btn btn-info btn-sm\"\n                  v-b-modal.game-update-modal\n                  @click=\"editGame(game)\"> Update </button>\n                  <button type=\"button\" class=\"btn btn-danger btn-sm\" @click=\"deleteGame(game)\">Delete</button>\n                </div>\n              </td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n    </div>\n\n  </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"GreenhouseDashboard\"\n}\n</script>\n\n<style scoped>\n\n</style>"],"mappings":"AA0EA;EACAA,IAAA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}